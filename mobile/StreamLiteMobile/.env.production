# StreamLite Production Environment Configuration
# This file overrides .env for production environment

# Environment type
EXPO_PUBLIC_ENVIRONMENT=production

# Production API Service URLs (replace with actual production domains)
EXPO_PUBLIC_AUTH_SERVICE_URL=https://api.streamlite.com/auth
EXPO_PUBLIC_USER_SERVICE_URL=https://api.streamlite.com/user
EXPO_PUBLIC_VIDEO_SERVICE_URL=https://api.streamlite.com/video

# Production timeouts (optimized for performance)
EXPO_PUBLIC_API_TIMEOUT=8000
EXPO_PUBLIC_UPLOAD_TIMEOUT=30000

# Disable debugging in production
EXPO_PUBLIC_DEBUG_API=false
EXPO_PUBLIC_DEBUG_CONSOLE=false
